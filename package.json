{
  "name": "bhasha-check-api",
  "version": "1.0.0",
  "description": "Node.js/Express API for BhashaCheck voice data management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "import": "node scripts/importCSV.js",
    "create-admin": "node scripts/createAdmin.js",
    "client:install": "cd client && npm install",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "build": "npm run client:build"
  },
  "keywords": [
    "express",
    "mongodb",
    "voice",
    "rsml",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "csv-parser": "^3.0.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
